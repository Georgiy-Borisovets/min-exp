{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "globals": {
    "screen": true,
    "require": true,
    "window": true,
    "document": true,
    "localStorage": true,
    "navigator": true,
    "fetch": true,
    "FormData": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src", "node_modules"]
      }
    }
  },
  "rules":{
    "react/jsx-props-no-spreading": "off"
  }
}
